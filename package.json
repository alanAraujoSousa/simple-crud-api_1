    "mongodb": "^3.2.7",
    "mongoose": "^5.6.9",
    "morgan": "^1.10.0",
    "node-uuid": "^1.4.8",
    "rootpath": "^0.1.2"
  },
  "devDependencies": {
    "jest": "^24.9.0",
    "jshint": "^2.11.0-rc1",
    "nodemon": "^1.19.1",
    "supertest": "^4.0.2"
