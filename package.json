  "version": "1.0.0",
  "description": "A simple API to create, read, update and delete entities",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alanAraujoSousa/simple-crud-api_1"
  },
  "keywords": [
    "api"
  ],
  "author": "Alan Araujo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/alanAraujoSousa/simple-crud-api_1/issues"
  },
  "homepage": "https://github.com/alanAraujoSousa/simple-crud-api_1#readme",
  "jshintConfig": {
    "esversion": 8,
    "eqeqeq": true,
    "strict": true
